{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "adjustFontSize",
        "delta": -1
      },
      "keys": "ctrl+minus"
    },
    {
      "command": "duplicateTab",
      "keys": "ctrl+shift+d"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "ctrl+alt+a"
    },
    {
      "command": {
        "action": "adjustFontSize",
        "delta": 1
      },
      "keys": "ctrl+plus"
    },
    {
      "command": {
        "action": "nextTab"
      },
      "keys": "ctrl+pgdn"
    },
    {
      "command": "resetFontSize",
      "keys": "ctrl+0"
    },
    {
      "command": {
        "action": "prevTab"
      },
      "keys": "ctrl+pgup"
    }
  ],
  "centerOnLaunch": true,
  "copyFormatting": "none",
  "copyOnSelect": false,
